    Config.Enabled = not Config.Enabled
    if Config.Enabled then
        ActivateBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        ActivateBtn.Text = "â›” DEACTIVATE"
        StatusLbl.Text = "ğŸ”¥ Status: MULTIPLYING!"
